{
    "manifest_version": 3,
    "name": "PriceCharting Pokémon Search",
    "version": "1.0",
    "description": "Search for Pokémon cards on PriceCharting using context menu",
    "permissions": [
        "contextMenus",
        "storage"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "options_ui": {
        "page": "options.html"
    },
    "icons": {
        "512": "pricecharting.png"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "{3d9bb53d-cc16-4e42-aeb2-e53966ce886f}",
            "strict_min_version": "109.0"
        }
    }
}